{
  "name": "uezcompany-backend",
  "version": "2.0.0",
  "description": "Backend da empresa UEZ Company",
  "main": "src/index.ts",
  "scripts": {
    "migrate": "npx prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "build": "npx tsup ./src",
    "start": "node --env-file .env ./dist/index.js",
    "dev": "tsx --env-file .env --watch ./src/index.ts",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests --detectOpenHandles --forceExit",
    "test:watch": "jest --passWithNoTests --watch",
    "test:staged": "jest --passWithNoTests --forceExit",
    "test:push": "jest --passWithNoTests --coverage --forceExit"
  },
  "keywords": [],
  "author": "UEZ COMPANY",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@prisma/client": "^5.10.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.26.1",
    "fastify-socket.io": "5.0.0",
    "prisma": "^5.10.2",
    "socket.io": "^4.7.2",
    "stripe": "^14.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^10.8.0",
