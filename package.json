    "@fastify/jwt": "^8.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@prisma/client": "^5.13.0",
    "bcrypt": "^5.1.1",
    "fastify": "^4.26.1",
    "fastify-socket.io": "5.0.0",
    "fastify-type-provider-zod": "^2.0.0",
